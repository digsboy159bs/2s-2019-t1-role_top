@model ROLE_TOP.ViewModels.HistoricoViewModel
@{
    ViewData["Title"] = "Reservas - Role_Top";
    ViewData["H2"] = "Histórico de Reservas";
}


<main>
    <h2>@ViewData["H2"]
        
    </h2>
    <table>
        <thead>
            <tr>
                <th>Data</th>
                <th>Evento</th>
                
                <th>Preço</th>
            </tr>
        </thead>
        <tbody>
            @foreach( var reserva in Model.Reservas ) {
                <tr>
                    <td>@reserva.DataDaReserva</td>
                    <td>@reserva.Evento.Nome</td>
                    
                    <td>@reserva.PrecoTotal</td>
                </tr>
            }
        </tbody>
    </table>

</main>